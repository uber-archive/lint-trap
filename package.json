{
  "name": "lint-trap",
  "version": "0.4.13",
  "description": "JavaScript linter module for Uber projects",
  "author": "Andrew de Andrade <aandrade@uber.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/lint-trap"
  },
  "bugs": {
    "url": "https://github.com/uber/lint-trap/issues"
  },
  "main": "./lint-trap.js",
  "keywords": [
    "lint",
    "jshint",
    "jscs",
    "eslint",
    "uber"
  ],
  "bin": {
    "lint-trap": "./bin/lint-trap.js"
  },
  "dependencies": {
    "async": "~0.9.0",
    "chalk": "^0.5.1",
    "commondir": "0.0.1",
    "debug": "^2.1.1",
    "deep-extend": "~0.3.2",
    "dotty": "0.0.2",
    "editorconfig": "^0.12.1",
    "eslint": "~0.11.0",
    "event-stream": "^3.2.1",
    "extend": "^2.0.0",
    "find-parent-dir": "^0.3.0",
    "fstream-ignore": "~1.0.2",
    "jscs": "~1.9.0",
    "jshint": "~2.5.11",
    "jsonfile": "~2.0.0",
    "jsonstream2": "^1.1.0",
    "minimist": "~1.1.0",
    "npm-which": "^2.0.0",
    "partial": "0.0.3",
    "pluralize": "^1.1.0",
    "process": "^0.10.0",
    "text-table": "^0.2.0",
    "through2": "^0.6.3",
    "through2-spy": "^1.2.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "istanbul": "^0.3.5",
    "mkdirp": "^0.5.0",
    "mock-fs": "^2.3.2",
    "opn": "^1.0.1",
    "pre-commit": "0.0.9",
    "rimraf": "^2.2.8",
    "tape": "~3.0.0",
    "temp": "^0.8.1",
    "which": "^1.0.8"
  },
  "scripts": {
    "cover": "istanbul cover --report cobertura --print detail tape -- test/**/*.tape.js",
    "lint": "./bin/lint-trap.js",
    "test": "npm run lint && tape test/**/*.tape.js",
    "view-cover": "istanbul report html && opn ./coverage/index.html"
  },
  "pre-commit": [
    "test"
  ],
  "license": "MIT"
}
